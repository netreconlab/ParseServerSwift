    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This directs packets to the same host that generated them.  This is useful for testing",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "applications on a single host and for debugging.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The local context ID obtained with ",
              "type" : "text"
            },
            {
              "code" : "getLocalContextID(_:)",
              "type" : "codeVoice"
            },
            {
              "text" : " can be used for the same",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "purpose, but it is preferable to use ",
              "type" : "text"
            },
            {
              "code" : "local",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "This is equal to ",
              "type" : "text"
            },
            {
              "code" : "VMADDR_CID_LOCAL (1)",
              "type" : "codeVoice"
            },
            {
              "text" : " on platforms that define it.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "",
                  "type" : "text"
                },
                {
                  "code" : "VMADDR_CID_LOCAL (1)",
                  "type" : "codeVoice"
                },
                {
                  "text" : " is available from Linux 5.6. Its use is unsupported on",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "other platforms.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],

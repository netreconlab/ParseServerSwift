          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Explicitly sequential execution of each future returned by the mapping",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "closure; the next future does not being executing until the previous one",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "has yielded a success result.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "No further futures will be even partially executed if any one future",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "returns a failure result.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "orderedList"
        },
        {
          "inlineContent" : [
            {
              "text" : "Neither of these are provided by the original version of the method.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }

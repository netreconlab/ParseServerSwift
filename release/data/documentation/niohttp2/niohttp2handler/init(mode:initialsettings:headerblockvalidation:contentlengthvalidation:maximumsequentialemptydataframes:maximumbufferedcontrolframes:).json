          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The mode for this handler, client or server.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "mode"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The settings that will be advertised to the peer in the preamble. Defaults to ",
                  "type" : "text"
                },
                {
                  "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseServerSwift\/documentation\/NIOHTTP2\/nioDefaultSettings",
                  "isActive" : true,
                  "type" : "reference"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "initialSettings"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Whether to validate sent and received HTTP\/2 header blocks. Defaults to ",
                  "type" : "text"
                },
                {
                  "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseServerSwift\/documentation\/NIOHTTP2\/NIOHTTP2Handler\/ValidationState\/enabled",
                  "isActive" : true,
                  "type" : "reference"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "headerBlockValidation"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Whether to validate the content length of sent and received streams. Defaults to ",
                  "type" : "text"
                },
                {
                  "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseServerSwift\/documentation\/NIOHTTP2\/NIOHTTP2Handler\/ValidationState\/enabled",
                  "isActive" : true,
                  "type" : "reference"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "contentLengthValidation"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Controls the number of empty data frames this handler will tolerate receiving in a row before DoS protection",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "is triggered and the connection is terminated. Defaults to 1.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "maximumSequentialEmptyDataFrames"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Controls the maximum buffer size of buffered outbound control frames. If we are unable to send control frames as",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "fast as we produce them we risk building up an unbounded buffer and exhausting our memory. To protect against this DoS vector, we put an",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "upper limit on the depth of this queue. Defaults to 10,000.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "maximumBufferedControlFrames"
        }
      ]

            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "will send a ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/edu.uky.cs.netreconlab.ParseServerSwift\/documentation\/NIOHTTP1\/HTTPParserError\/invalidHeaderToken",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",

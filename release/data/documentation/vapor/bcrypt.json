        {
          "inlineContent" : [
            {
              "text" : "Use BCrypt to create hashes for sensitive information like passwords.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "try BCrypt.hash(\"vapor\", cost: 4)"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "BCrypt uses a random salt each time it creates a hash. To verify hashes, use the ",
              "type" : "text"
            },
            {
              "code" : "verify(_:matches)",
              "type" : "codeVoice"
            },
            {
              "text" : " method.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let hash = try BCrypt.hash(\"vapor\", cost: 4)",
            "try BCrypt.verify(\"vapor\", created: hash) \/\/ true"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {

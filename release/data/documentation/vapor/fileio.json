    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "It can read files, both in their entirety and chunked.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let fileio = try c.make(FileIO.self)",
            "",
            "fileio.readFile(at: \"\/path\/to\/file.txt\") { chunk in",
            "    print(chunk) \/\/ part of file",
            "}",
            "",
            "fileio.collectFile(at: \"\/path\/to\/file.txt\").map { file in",
            "    print(file) \/\/ entire file",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "It can also create streaming HTTP responses.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let fileio = try c.make(FileIO.self)",
            "router.get(\"file-stream\") { req -> Response in",
            "    return fileio.streamFile(at: \"\/path\/to\/file.txt\", for: req)",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Streaming file responses respect ",
              "type" : "text"
            },
            {
              "code" : "E-Tag",
              "type" : "codeVoice"
            },
            {
              "text" : " headers present in the request.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }

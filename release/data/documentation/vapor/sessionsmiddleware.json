        },
        {
          "inlineContent" : [
            {
              "text" : "If a cookie matching the configured cookie name is found on an incoming request,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the value will be used as an identifier to find the associated ",
              "type" : "text"
            },
            {
              "code" : "Session",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If a session is used during a request (",
              "type" : "text"
            },
            {
              "code" : "Request.session()",
              "type" : "codeVoice"
            },
            {
              "text" : "), a cookie will be set",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "on the outgoing response with the sessionâ€™s unique identifier. This cookie must be",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "returned on the next request to restore the session.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "app.middleware.use(app.sessions.middleware)"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "See ",
              "type" : "text"
            },
            {
              "code" : "SessionsConfig",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "Sessions",

        },
        {
          "inlineContent" : [
            {
              "text" : "Most encoders should implement this method by simply forwarding it to the encoder userInfo-aware version below,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "e.g. ",
              "type" : "text"
            },
            {
              "code" : "try self.encode(encodable, to: &body, headers: &headers, userInfo: [:])",
              "type" : "codeVoice"
            },
            {
              "text" : ". For legacy API compatibility",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "reasons, the default protocol conformance will do the exact opposite.",
              "type" : "text"
            }
          ],
